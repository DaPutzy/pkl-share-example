import ".../templating/apps/TenantConfig.pkl"

tenants: Listing<TenantConfig.Item>(length > 0) = new Listing<TenantConfig.Item> {}

templates: Mapping<String, TenantConfig>

output {
  files {
    for (path, template in templates) {
      [path] = (template) {
        tenants = module.tenants
      }.output
    }
  }
}
