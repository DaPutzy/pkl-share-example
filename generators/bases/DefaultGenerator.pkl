import "BaseTemplate.pkl"
import "TenantConfig.pkl"

warning = """
  ####################################################################################
  # ⚠️ THIS IS A GENERATED FILE ⚠️
  # DO NOT MAKE MANUAL CHANGES
  ####################################################################################


  """

function generate(
  input: BaseTemplate,
  tenantConfig: TenantConfig
): FileOutput = new {
  local template = (input) {
    tenants = tenantConfig.tenants
  }

  value = template.output.value
  renderer = template.output.renderer
  text = if (template.output.renderer.extension == "yaml") warning + super.text else super.text
}
