extends ".../TenantConfig.pkl"

redirectUris: Listing<Uri>?

fixed filename: "keycloak_client"

output {
  value = new Listing {
    for (tenant in module.tenants) {
      new {
        apiVersion = "keycloak.org/v1alpha1"
        kind = "SimplifiedKeycloakClientForThisExampleProject"
        name = "different-app-keycloak-client-" + tenant.name
        realm = tenant.name + "-realm"
        clientId = "different-app-cloud"
        when (redirectUris != null) {
          redirectUris = module.redirectUris
        }
      }
    }
  }
  renderer = new YamlRenderer {
    isStream = true
  }
}
