import ".../templating/apps/TenantConfig.pkl"
import ".../templating/apps/TenantConfigItem.pkl"

tenants: Listing<TenantConfigItem>(length > 0) = new Listing<TenantConfigItem> {}

templates: Mapping<String, TenantConfig>

output {
  files {
    for (path, template in templates) {
      [path] = (template) {
        tenants = module.tenants
      }.output
    }
  }
}
